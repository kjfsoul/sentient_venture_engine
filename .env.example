# Supabase Core Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# OpenAI API Configuration
OPENAI_API_KEY=sk-your_openai_api_key_here

# OpenRouter API (for cost-effective models like Qwen 3, Deepseek, Minimax)
OPENROUTER_API_KEY=sk-or-v1-your_openrouter_api_key_here

# Google AI Studio / Gemini API
GEMINI_API_KEY=your_gemini_api_key_here

# RouteLL API
ROUTELLM_API_KEY=your_routellm_api_key_here

# DeepAI API
DEEPAI_API_KEY=your_deepai_api_key_here

# Pexels API (for image search)
PEXELS_API_KEY=your_pexels_api_key_here

# Together.ai API
TOGETHER_API_KEY=tgp_v1_your_together_api_key_here

# Groq API
GROQ_API_KEY=gsk_your_groq_api_key_here

# Hugging Face API
HF_API_KEY=hf_your_huggingface_api_key_here

